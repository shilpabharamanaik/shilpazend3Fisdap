					<script>
						var stopwatchIntervalId = null;
						
						var secondsSpent = 0;

						var isPaused = false;
						
						document.getElementById("stopwatch-pause-button").style.display = 'none';

						function renderStopwatchTime(){
							tmpTime = secondsSpent;
							
							hours = Math.floor(tmpTime/3600);
							if(hours <= 9){ hours = '0' + hours; }
							tmpTime = tmpTime - (hours*3600);
							
							minutes = Math.floor(tmpTime/60);
							if(minutes <= 9){ minutes = '0' + minutes; }
							tmpTime = tmpTime - (minutes*60);
							
							seconds = tmpTime;
							if(seconds <= 9){ seconds = '0' + seconds; }

							document.getElementById("stopwatch-time").innerHTML = hours + ':' + minutes + ':' + seconds;
						}

						function startStopwatch(){
							document.getElementById("stopwatch-start-button").style.display = 'none';
							document.getElementById("stopwatch-pause-button").style.display = 'inline';
							stopwatchIntervalId = setInterval(function(){
								secondsSpent++;
								renderStopwatchTime();
								isPaused = false;
							}, 1000);
						}
						
						function pauseStopwatch(){
							if(isPaused){
								startStopwatch();
								document.getElementById("stopwatch-pause-button").value = 'Pause';
								$('#stopwatch-pause-button').removeClass('green-button');
								isPaused = false;
							}else{
								isPaused = true;
								document.getElementById("stopwatch-pause-button").value = 'Resume';
								$('#stopwatch-pause-button').addClass('green-button');
								clearInterval(stopwatchIntervalId);
							}
						}
						
						function resetStopwatch(){
							document.getElementById("stopwatch-start-button").style.display = 'inline';
							document.getElementById("stopwatch-pause-button").style.display = 'none';
							clearInterval(stopwatchIntervalId);
							secondsSpent = 0;
							
							renderStopwatchTime();

							stopwatchIntervalId = null;
						}
					</script>
