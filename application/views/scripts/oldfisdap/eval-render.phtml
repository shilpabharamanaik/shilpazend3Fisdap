			// Set up the tabs and buttonsets...
			$(function(){
				$('#tabs').tabs();
				$('.eval-item-options').buttonset();
			});
			
			// Set up the scroll positioning of the stopwatch...
			$(function(){
				topDiv = $('#stopwatch-top');
				bottomDiv = $('#stopwatch-bottom');
				
				stopwatchDiv = $('#stopwatch');
				
				// Figure out what the absolute maximum and minimum heights are for the stopwatch.
				maxTop = topDiv.offset().top + topDiv.height();
				maxBottom = bottomDiv.offset().top + bottomDiv.height() - stopwatchDiv.height();
				
				// Default it to the top of the div
				stopwatchDiv.offset({top: maxTop, left: stopwatchDiv.left});
				
				// When the user scrolls, set up the stopwatch div to lock to the top of the screen.
				$(document).scroll(function(e){
					// Update these here just in case the heights change between scrolls (user can adjust
					// the height of the textarea in some browsers).
					maxTop = topDiv.offset().top + topDiv.height();
					maxBottom = bottomDiv.offset().top + bottomDiv.height() - stopwatchDiv.height();
					
					stopwatchPos = stopwatchDiv.offset();
					
					if(window.pageYOffset > topDiv.offset().top){
						stopwatchDiv.offset({top: Math.min(window.pageYOffset, maxBottom), left: stopwatchDiv.left});
					}else{
						stopwatchDiv.offset({top: maxTop, left: stopwatchDiv.left});
					}
				});
			});
			
			var startTime = null;
			var endTime = null;
				
			function updateTotalTimeDisplay(){
				if(startTime != null && endTime != null && startTime.getTime() < endTime.getTime()){
					secondsTaken = Math.floor(((endTime.getTime() - startTime.getTime()) / 1000));
					
					hasHours = false;
					
					hours = Math.floor(secondsTaken/3600);
					if(hours > 0){
						hasHours = true;
					}
					if(hours <= 9){ hours = '0' + hours;}
					secondsTaken = secondsTaken - (hours*3600);
					
					minutes = Math.floor(secondsTaken/60);
					if(minutes <= 9){ minutes = '0' + minutes; }
					secondsTaken = secondsTaken - (minutes*60);
					
					seconds = secondsTaken;
					if(seconds <= 9){ seconds = '0' + seconds; }
					
					if(hasHours){
						$('#total-time').text(hours + 'h ' + minutes + 'm ' + seconds + 's');
					}else{
						$('#total-time').text(minutes + 'm ' + seconds + 's');
					}
				}else{
					$('#total-time').text('00m 00s');
				}
			}
			
			function updateTime(element){
				curTime = new Date();

				hourTime = curTime.getHours();
				if(hourTime <= 9){ hourTime = '0' + hourTime; }

				minTime = curTime.getMinutes();
				if(minTime <= 9){ minTime = '0' + minTime; }
				
				$('#' + element).val(hourTime + '' + minTime);
				
				updateTotalTimeDisplay();
			}
			
			// Set up the start/end time boxes.
			$(function(){
				$("#datepicker").datepicker();
				
				$("#set-current-start-time").click(function(){
					startTime = new Date();
					updateTime('time_start');
				});
				$("#set-current-end-time").click(function(){
					endTime = new Date();
					updateTime('time_end');
				});
				
				startTime = new Date();
				updateTime('time_start');
			});
			
			// Set up the end time button
			$(function(){
				var timerId = null;
				
				toggleTimer = function(){
					if(timerId == null){
						timerId = setInterval(function(){
							endTime = new Date();
							updateTime('time_end');
						}, 1000);
						
						document.getElementById("time_end").disabled = true;
						document.getElementById("timerToggleButton").value = 'Turn Timer Off';
					}else{
						clearInterval(timerId);
						timerId = null;
						document.getElementById("time_end").disabled = false;
						document.getElementById("timerToggleButton").value = 'Turn Timer On';
					}
				}
			});
			
			// Set up the logic to handle the radio button clicks...
			$(function(){
				// Set the cutscore to be a static 70%
				var cutscore = 70;
				$('#cutscore').text(cutscore);
				$('#current-score').text(0).css('color', '#FF0000');
				
				var pointCount = 0;
				
				updateEarnedPoints = function(){
					var earnedPoints = 0;
					$('.eval-item-options').each(function(index, el){
						$(el).children().each(function(index, radioEl){
							if($(radioEl).is("input")){
								if($(radioEl).is(':checked') && $(radioEl).val() >= 0){
									earnedPoints += parseInt($(radioEl).val());
								}
							}
						});	
					});
					
					earnedScore = Math.floor((earnedPoints/pointCount*100));
					
					$('#current-score').text(earnedScore);
					
					if(earnedScore >= cutscore){
						$('#current-score').css('color', '#00FF00');	
					}else{
						$('#current-score').css('color', '#FF0000');	
					}					
					
					$('#earned-points').text(earnedPoints);
					$('#earned-points-2').text(earnedPoints);
				}
				
				$('.eval-item-options').each(function(index, el){
					$(el).children().each(function(index, radioEl){
						if($(radioEl).is("input")){
							if($(radioEl).val() >= 0){
								// Set up a click handler on each to recalculate the current number of earned points...
								$(radioEl).click(updateEarnedPoints);
								pointCount++;
							}
						}
					});	
				});
				
				$('#possible-points').text(pointCount);
				$('#possible-points-2').text(pointCount);
			});
			
			// Make the pretty pretty buttons...
			$(function(){
				$('#submitButton').button().addClass('green-button medium');
