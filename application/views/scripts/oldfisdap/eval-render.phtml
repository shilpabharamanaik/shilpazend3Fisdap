						<input type='button' value='Start' id='stopwatch-start-button' onclick='startStopwatch()' />
						<input type='button' value='Pause' id='stopwatch-pause-button' onclick='pauseStopwatch()' />
						<input type='button' value='Reset' id='stopwatch-stop-button' onclick='resetStopwatch()' /><br />
