{
  "name": "fisdap/attachments",
  "description": "Library providing file attachment functionality for Doctrine Entities",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Ben Getsug",
      "email": "bgetsug@fisdap.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fisdap\\Attachments\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://toran.fisdap.net/repo/private/"
    },
    {
      "type": "composer",
      "url": "https://toran.fisdap.net/repo/packagist/"
    },
    {"packagist": false}
  ],
  "require": {
    "php": ">=5.5",
    "intervention/image": "^2.3.2",
    "hoa/mime": "3.17.01.10",
    "hoa/file": "1.17.07.11",
    "aws/aws-sdk-php": "~3.0",
    "aws/aws-sdk-php-laravel": "^3.0.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "fisdap/data-repository": "dev-master",
    "fisdap/doctrine-entity-timestamps": "dev-master",
    "fisdap/laravel-console-extensions": "^1.0",
    "fisdap/logging": "dev-master",
    "fisdap/laravel-error-handling": "dev-master",
    "fisdap/fractal-extensions": "dev-master",
    "fisdap/doctrine-extensions": "dev-master",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "laravel/framework": "5.5.*",
    "monolog/monolog": "@stable",
    "codeception/codeception": "2.*",
    "codeception/mockery-module": "0.2.*"
  }
}
