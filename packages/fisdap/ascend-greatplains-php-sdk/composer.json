{
    "name": "fisdap/ascend-greatplains-php-sdk",
    "description": "PHP SDK to connect to Ascend Greatplains API",
    "keywords": ["fisdap", "ascend", "greatplains"],
    "license": "Closed",
    "type": "library",
    "authors": [
        {
            "name": "Jason Michels",
            "email": "jmichels@fisdap.net"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phing/phing": "^2.13",
        "mockery/mockery": "^0.9.4",
        "halleck45/php-metrics": "^1.8",
        "squizlabs/php_codesniffer": "^2.5",
        "sebastian/phpcpd": "^2.0",
        "phpmd/phpmd": "^2.3",
        "phpdocumentor/phpdocumentor": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Fisdap\\Ascend\\Greatplains\\": "src",
            "Fisdap\\Ascend\\Greatplains\\Phpunit\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
