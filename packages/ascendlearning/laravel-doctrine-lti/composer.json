{
    "name": "ascendlearning/laravel-doctrine-lti",
	"version": "dev-master",
    "description": "A library providing LTI domain entities (for use with Doctrine ORM) and Laravel console commands",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ben Getsug",
            "email": "bgetsug@fisdap.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://toran.fisdap.net/repo/private/"
        }
    ],
    "require": {
        "franzl/lti": "dev-master",
        "fisdap/data-repository": "@stable",
        "fisdap/doctrine-extensions": "@stable"
    },
    "require-dev": {
        "illuminate/support": "~5",
        "illuminate/console": "~5"
    },
    "autoload": {
        "psr-4": {
            "AscendLearning\\Lti\\": "src/"
        }
    }
}
